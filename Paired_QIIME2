################################											
											
# 2. Import data											
											
"# cd to Desktop folder named ""Core"""											
											
cd ~/Desktop/Core_Seqs											
											
source activate qiime2-2019.10											
											
qiime tools import 	--type 'SampleData[PairedEndSequencesWithQuality]' 	--input-path pe-33-manifest_EAB.csv 	--output-path EAB_pe_demux.qza 	--input-format PairedEndFastqManifestPhred33											
  											
cd ~/Desktop/Core

qiime demux summarize --i-data EAB_pe_demux.qza --o-visualization EAB_pe_demux.qzv											
  											
#Look at visualization to determine trim and trunc											
#trim = 2nd percentile quality score											
#truc = sequence length summary - 1											
											
qiime dada2 denoise-paired 	--i-demultiplexed-seqs EAB_pe_demux.qza   --p-trim-left-f 12  --p-trim-left-r 12  --p-trunc-len-f 249 --p-trunc-len-r 249  --o-table EAB_table.qza  --o-representative-sequences EAB_rep-seqs.qza --o-denoising-stats EAB_denoising-stats.qza											
  											
qiime feature-table summarize  --i-table EAB_table.qza  --o-visualization EAB_table.qzv  --m-sample-metadata-file EAB_Paired_Map											
											
qiime feature-table tabulate-seqs  --i-data EAB_rep-seqs.qza  --o-visualization EAB_rep-seqs.qzv											
											
qiime metadata tabulate  --m-input-file EAB_denoising-stats.qza  --o-visualization EAB_denoising-stats.qzv											
  											
qiime phylogeny align-to-tree-mafft-fasttree  --i-sequences EAB_rep-seqs.qza --o-alignment EAB_aligned-rep-seqs.qza  --o-masked-alignment EAB_masked-aligned-rep-seqs.qza  --o-tree EAB_unrooted-tree.qza --o-rooted-tree EAB_rooted-tree.qza											
  											  												
#import green genes sequences
											
qiime tools import --input-path /usr/local/MacQIIME_1.9.1-20150604_OS10.7/macqiime/anaconda/lib/python2.7/site-packages/qiime_default_reference/gg_13_8_otus/rep_set/97_otus.fasta --type 'FeatureData[Sequence]'  --output-path 97_otus.qza											
											
#inport taxonomy		
									
qiime tools import --input-path /usr/local/MacQIIME_1.9.1-20150604_OS10.7/macqiime/anaconda/lib/python2.7/site-packages/qiime_default_reference/gg_13_8_otus/taxonomy/97_otu_taxonomy.txt  --type 'FeatureData[Taxonomy]' --input-format HeaderlessTSVTaxonomyFormat --output-path ref_taxonomy.qza											
											
#Classify taxonomy											
qiime feature-classifier classify-sklearn --i-classifier gg-13-8-99-515-806-nb-classifier.qza --i-reads EAB_rep-seqs.qza --o-classification EAB_taxonomy.qza											
  											
#filter out chloroplast and mitochondria

qiime feature-table filter-features \
  --i-table EAB_table.qza \
  --m-metadata-file EAB_taxonomy.qza \
  --p-where "Taxon NOT LIKE '%Chloroplast%'" \
  --o-filtered-table EAB_filtered-table-sans-chloroplast.qza \
  
qiime feature-table filter-features \
  --i-table EAB_filtered-table-sans-chloroplast.qza \
  --m-metadata-file EAB_taxonomy.qza \
  --p-where "Taxon NOT LIKE '%mitochondria%'" \
  --o-filtered-table EAB_filtered-table-sans-chloromito.qza \
  
qiime feature-table filter-seqs \
  --i-data EAB_rep-seqs.qza \
  --i-table EAB_filtered-table-sans-chloromito.qza \
  --o-filtered-data EAB_filtered-seqs-sans-chloroplast.qza
  
#re-run tree and rarefy

qiime phylogeny align-to-tree-mafft-fasttree  --i-sequences EAB_filtered-seqs-sans-chloroplast.qza --o-alignment EAB_aligned-rep-seqs_nchl.qza  --o-masked-alignment EAB_masked-aligned-rep-seqs_nchl.qza  --o-tree EAB_unrooted-tree_nchl.qza --o-rooted-tree EAB_rooted-tree_nchl.qza											
  											
qiime diversity alpha-rarefaction --i-table EAB_filtered-table-sans-chloromito.qza --i-phylogeny EAB_rooted-tree_nchl.qza --p-metrics chao1 --p-metrics shannon --p-metrics faith_pd --p-metrics observed_otus --p-max-depth 4000 --p-steps 40 --m-metadata-file EAB_Paired_Map.txt --o-visualization EAB_alpha-rarefaction_nchl.qzv											
											
#download csv files	
#5 samples failed, to keep all the rest of the samples, rarefy to 1100																				
											
qiime diversity core-metrics-phylogenetic --i-phylogeny EAB_rooted-tree_nchl.qza --i-table EAB_filtered-table-sans-chloromito.qza --p-sampling-depth 1100 --m-metadata-file EAB_Paired_Map.txt --output-dir EAB_core-metrics-results_nchl											
  
#filter rarefied feature table to delete singletons	
										
qiime feature-table filter-samples --i-table EAB_core-metrics-results_nchl/rarefied_table.qza --p-min-frequency 2 --o-filtered-table EAB_table_rare_ns_nchl.qza											
  											
qiime feature-table filter-seqs --i-data EAB_filtered-seqs-sans-chloroplast.qza --i-table EAB_table_rare_ns_nchl.qza --o-filtered-data EAB_filtered-seqs_nchl.qza											
																			
#Classify taxonomy											
qiime feature-classifier classify-sklearn --i-classifier gg-13-8-99-515-806-nb-classifier.qza --i-reads EAB_filtered-seqs_nchl.qza --o-classification EAB_taxonomy_nchl.qza											

qiime metadata tabulate --m-input-file EAB_taxonomy_nchl.qza  --o-visualization EAB_taxonomy.qzv											
											
qiime taxa collapse --i-table EAB_table_rare_ns_nchl.qza --i-taxonomy EAB_taxonomy_nchl.qza --p-level 5 --o-collapsed-table EAB_table_f.qza											
  											
qiime taxa collapse --i-table EAB_table_rare_ns_nchl.qza --i-taxonomy EAB_taxonomy_nchl.qza --p-level 2 --o-collapsed-table EAB_table_p.qza											
  											
qiime tools export --input-path EAB_table_rare_ns_nchl.qza --output-path EAB_exported-feature-table											
											
biom convert -i EAB_exported-feature-table/feature-table.biom -o EAB_otu_table.txt --to-tsv											
											
qiime tools export --input-path EAB_taxonomy_nchl.qza --output-path EAB_exported-taxonomy/											
											
qiime tools export --input-path EAB_table_f.qza --output-path EAB_exported-taxonomyf											
  											
biom convert -i EAB_exported-taxonomyf/feature-table.biom -o EAB_otu_table_f.txt --to-tsv											
  											
qiime tools export --input-path EAB_table_p.qza --output-path EAB_exported-taxonomyp											
  											
biom convert -i EAB_exported-taxonomyp/feature-table.biom -o EAB_otu_table_p.txt --to-tsv											
  											
qiime tools export --input-path EAB_core-metrics-results_nchl/weighted_unifrac_distance_matrix.qza --output-path EAB_exported-wunifrac											
											
qiime tools export  --input-path EAB_core-metrics-results_nchl/faith_pd_vector.qza --output-path EAB_exported-faithpd											
											
qiime diversity alpha  --i-table EAB_table_rare_ns_nchl.qza  --p-metric chao1 --o-alpha-diversity EAB_chao1.qza											
											
qiime tools export --input-path EAB_chao1.qza --output-path EAB_exported-chao1
